            global.dbg_huge_hp_mode = selected;
        } else if (text == "KEEP MOVING") {
            global.dbg_keep_moving = selected;
        } else if (text == "NORMAL MODE") {
             global.dbg_powerup_events = selected;
             global.dbg_meteorite_events = selected;
        }
    }
}
